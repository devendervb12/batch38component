<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="smax.batch38.Product.controller.Page2"
		xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Page2">
				<content>
				<VBox>
				
				   <Text text="{ProductID}"/>
				   <Text text="{Name}"/>
				   <Text text="{Description}"/>
				   <Text text="{Category}"/> 
				  </VBox> 
					<Button text="Go to Page1" press="gotoPage1" />
				</content>
			</Page>
</core:View>